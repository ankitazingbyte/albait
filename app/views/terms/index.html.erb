<p id="notice"><%= notice %></p>

<h1>Terms</h1>

<table>
  <thead>
    <tr>
      <th>First term</th>
      <th>Second term</th>
      <th>Third term</th>
      <th>Fourth term</th>
      <th>Fivth term</th>
      <th>Sixth term</th>
      <th>Seventh term</th>
      <th>Eight term</th>
      <th>Ninth term</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @terms.each do |term| %>
      <tr>
        <td><%= term.first_term %></td>
        <td><%= term.second_term %></td>
        <td><%= term.third_term %></td>
        <td><%= term.fourth_term %></td>
        <td><%= term.fivth_term %></td>
        <td><%= term.sixth_term %></td>
        <td><%= term.seventh_term %></td>
        <td><%= term.eight_term %></td>
        <td><%= term.ninth_term %></td>
        <td><%= link_to 'Show', term %></td>
        <td><%= link_to 'Edit', edit_term_path(term) %></td>
        <td><%= link_to 'Destroy', term, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Term', new_term_path %>
